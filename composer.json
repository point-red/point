{
    "name": "point-red/point",
    "description": "Point Framework.",
    "keywords": ["framework", "point", "erp"],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "~4.0",
        "google/cloud-firestore": "^1.6",
        "grpc/grpc": "^v1.1.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^1.4",
        "laravel/passport": "^7.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "^3.1",
        "point-red/laravel-database-management": "^1.0",
        "rollbar/rollbar-laravel": "^4.0",
        "spatie/laravel-permission": "^2.12"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^2.0",
        "php-coveralls/php-coveralls": "dev-master",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
